{
  "name": "amqp-consumers",
  "version": "1.0.5",
  "description": "\"Amqp server with typescript decorators\"",
  "main": "bin/main.js",
  "scripts": {
    "start": "npm run build && node --inspect=0.0.0.0:5858 ./bin/main.js",
    "dev": "nodemon",
    "build": "./node_modules/typescript/bin/tsc -p tsconfig.json",
    "build:watch": "./node_modules/typescript/bin/tsc -w -p tsconfig.json",
    "lint": "./node_modules/tslint/bin/tslint -t stylish --project \"tsconfig.json\"",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts",
    "clean": "./node_modules/rimraf/bin.js bin"
  },
  "keywords": ["amqp", "typescript", "rabbitmq", "decorators"],
  "author": "Elis Vathi",
  "license": "MIT",
  
  "dependencies": {
    "amqplib": "^0.5.3",
    "reflect-metadata": "^0.1.12",
    "tslib": "^1.9.3",
    "typescript": "^3.2.2",
    "uuid": "^3.3.2"
  },
  "bin": {
    "amqp-consumers": "bin/main.js"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.9",
    "@types/uuid": "^3.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elisvathi/amqp-consumers.git"
  },
  "bugs": {
    "url": "https://github.com/elisvathi/amqp-consumers/issues"
  },
  "homepage": "https://github.com/elisvathi/amqp-consumers#readme"
}
